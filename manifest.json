{
  "name": "Web Arranger",
  "version": "1.0",
  "description": "Make the web accessible!",
  "permissions": ["declarativeContent","storage","activeTab"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2,

  "icons": {
    "48": "border-48.png",
    "96": "border-96.png"
  }
}